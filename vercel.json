{
    "version": 2,
    "builds": [
        {
            "src":"./index.js",
            "use":"@vercel/node",
            "build":"npx tailwindcss -i ./input.css -o ./public/stylesheets/main.css"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}