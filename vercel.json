{
    "version": 2,
    "builds": [
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "buildCommand": "npx tailwindcss -i ./input.css -o ./public/stylesheets/main.css",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}